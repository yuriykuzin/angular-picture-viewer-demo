{
  "extends": "google",

  "env": {
    "es6": true,
    "browser": true,
    "jquery": true
  },

  "parserOptions": {
    "sourceType": "module"
  },

  "globals": {
    "require": true,
    "module": true,
    "angular": true,
    "describe": true,
    "it": true,
    "expect": true,
    "beforeEach": true,
    "inject": true,
    "spyOn": true,
    "jasmine": true
  },

  "rules": {
    // Override any settings from the "parent" configuration
	"semi": [2, "always"],
    "no-unused-vars": [1, {"vars": "all", "args": "after-used"}],
    "max-len": [2, 120, 2, {
      "ignoreComments": true,
      "ignoreUrls": true
    }],
    "eol-last": 0,
    "one-var-declaration-per-line": 0,
    "one-var": 0,
    "no-useless-constructor": 0,
    "arrow-parens": 0,
    "no-return-assign": 0,
    "no-warning-comments": 0,
    "no-implicit-coercion": 0,
    "no-negated-condition": 0,
    "require-jsdoc": 0,
    "new-cap": 0,
    "valid-jsdoc": 0
  }
}
